    <validationRules>
        <fullName>Age_check</fullName>
        <active>true</active>
        <description>age should be between 21-30</description>
        <errorConditionFormula>OR(Age__c &lt;20,Age__c &gt; 31)</errorConditionFormula>
        <errorMessage>Age should be between 21 and 30</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Bank_detail</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( $ObjectType.Salary_Details__c.Fields.Bank_Details__c )</errorConditionFormula>
        <errorMessage>Bank account details cannot be null.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DOB</fullName>
        <active>true</active>
        <errorConditionFormula>Age__c &lt; 18</errorConditionFormula>
        <errorDisplayField>DateOfBirth__c</errorDisplayField>
        <errorMessage>Please provide a valid Date of birth. Employee cannot be less than 18 years of Age.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Val_for_last_working_day</fullName>
        <active>true</active>
        <errorConditionFormula>Last_Working_Day__c &lt;  Date_of_Joining__c</errorConditionFormula>
        <errorMessage>Last working date should be greater than joining date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>notice_check</fullName>
        <active>true</active>
        <errorConditionFormula>AND( Is_on_Notice_period__c , ISBLANK( Resignation_Date__c ) )</errorConditionFormula>
        <errorDisplayField>Resignation_Date__c</errorDisplayField>
        <errorMessage>Resignation date cannot be null</errorMessage>
    </validationRules>
